module "population"
load "../ant-colony.pm"

set "initial_larvae" 20.0
set "initial_nurses" 100.0
set "initial_forager" 100.0
set "temperature" 5.0
set "foodAvailabilityRate" 0.5

init "initial"

add all measures
deadline 100
dt 1.0
replica 10
simulate
save output "./results/20-100-100-5-0.5" prefix "ac-old_colony-lT-hF-summary_" postfix "__"