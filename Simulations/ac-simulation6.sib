module "population"
load "../ant-colony.pm"

set "initial_larvae" 20.0
set "initial_nurses" 10.0
set "initial_forager" 10.0
set "temperature" 5.0
set "foodAvailabilityRate" 0.5

init "initial"

add all measures
deadline 100
dt 1.0
replica 10
simulate
save output "./results/20-10-10-5-0.5" prefix "nc-lT-mF-summary_" postfix "__"