module "population"
load "../ant-colony.pm"
add all measures
deadline 100
dt 1.0
replica 10

set "initial_larvae" 20.0
set "initial_nurses" 100.0
set "initial_forager" 100.0
set "temperature" 25.0
set "foodAvailabilityRate" 1.0

init "initial"
simulate
save output "./results/20-100-100-25-1" prefix "ac-old_colony-middle_temperature-summary_" postfix "__"