module "population"
load "../ant-colony.pm"
add all measures
deadline 100
dt 1.0
replica 10

set "initial_larvae" 20.0
set "initial_nurses" 100.0
set "initial_forager" 100.0
set "temperature" 20.0
set "foodAvailabilityRate" 0.25

init "initial"
simulate
save output "./results/20-100-100-20-0.25" prefix "ac-old_colony-middle_temperature-low_food-summary_" postfix "__"